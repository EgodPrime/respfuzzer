[llm]
base_url = "https://api.openai.com/v1"
api_key = "sk-xxxxxxx" # your OpenAI API key
model_name = "gpt-4-0613"
temperature = 0.7
top_p = 0.8
top_k = 20
presence_penalty = 1.0

[db_config]
db_name = "tracefuzz" # without .db suffix

[redis]
host = "127.0.0.1"
port = 6379
db = 0

[fuzz]
execution_timeout = 5.0
mutants_per_seed = 100000
max_try_per_seed = 10

[reflective_seeder]
concurrency = 12 # number of threads for seeding
use_reasoner = true
use_docs = true

[fuzz4all]
strategy = 1
concurrency = 10
execution_timeout = 5.0
mutants_per_seed = 100